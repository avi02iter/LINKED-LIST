public class linked {   //central class 
    // this is the class for node
    public static class Node {
        int data;
        Node next;
    }

    // this is the class for overall linked list
    public static class list {   // this has to make the object of it
        Node head;
        Node tail;
        int size;

        void addLast(int value) {
            Node temp = new Node();
            temp.data = value;
            temp.next = null;
            if (size == 0) {
                head = tail = temp;
            } else {
                tail.next = temp;
                tail = temp;
            }
            size++;

        }

        void print() {
            Node temp = head;
            while (temp != null) {
                System.out.print(temp.data + "--> ");
                temp = temp.next;
            }

        }
    }

    public static void main(String[] args) {
        list li = new list();
        li.addLast(2);
        li.addLast(3);
        li.addLast(4);
        li.addLast(5);
        li.addLast(6);
        li.addLast(7);
        li.print();

    }
}
